{
  "name": "clickalicious/rng",
  "description": "Rng - The secure random number generator (PRNG) for PHP",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4.0",
    "paragonie/random_compat": "~2.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.24",
    "symfony/var-dumper": "~2.8.4"
  },
  "autoload": {
    "psr-4": {
      "Clickalicious\\": "src/Clickalicious/"
